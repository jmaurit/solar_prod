@article{devos_location_2014,
	title = {Location and lease intensity},
	volume = {29},
	issn = {0929-1199},
	url = {http://www.sciencedirect.com/science/article/pii/S0929119914000960},
	doi = {10.1016/j.jcorpfin.2014.08.005},
	abstract = {U.S. firms lease assets extensively. We find that, during 1980–2011, the average U.S. firm has a lease intensity of about 40\%. Or, the average firm has present and future (up to five years) rent commitments equal to 16.6\% of their total assets. We investigate whether agency costs between the lessor and the lessee affect the lease intensity of firms. To do so, we examine the impact of firms' location on the use of operating leases. The main idea of our paper is that, because obtaining information and monitoring is costly for potential lessors, especially when a lessee is relatively far away from financial centers, rural firms are less likely to use operating leases. Consistent with this hypothesis, we show that rural firms tend to have lower lease intensities than similar urban and small city firms. In addition, we find that firms with higher levels of debt capacity lease less and firms that face more financial constraint lease more. Our findings are robust to industry and lease maturity controls and consistent with the existence of an agency problem associated with leasing.},
	urldate = {2016-06-06},
	journal = {Journal of Corporate Finance},
	author = {Devos, Erik and Rahman, Shofiqur},
	month = dec,
	year = {2014},
	keywords = {Debt financing, Geographic location, Operating leases},
	pages = {20--36},
	file = {ScienceDirect Full Text PDF:/Users/johannesmauritzen/Library/Mobile Documents/com~apple~CloudDocs/zotero/storage/34EBSBSC/Devos and Rahman - 2014 - Location and lease intensity.pdf:application/pdf;ScienceDirect Snapshot:/Users/johannesmauritzen/Library/Mobile Documents/com~apple~CloudDocs/zotero/storage/DCC5TFVB/S0929119914000960.html:text/html}
}

