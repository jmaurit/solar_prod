}

@article{gilligan_lemons_2004,
	title = {Lemons and {Leases} in the {Used} {Business} {Aircraft} {Market}},
	volume = {112},
	issn = {0022-3808},
	url = {http://www.jstor.org/stable/10.1086/422561},
	doi = {10.1086/422561},
	abstract = {Given adverse selection, durable goods that trade less frequently depreciate more quickly. Consistent with this prediction, I find an inverse relationship between depreciation and trading volume for less reliable brands of used business aircraft. Additionally, recent theoretical analyses suggest that leasing, by increasing the average quality of used goods offered for sale, may reduce adverse selection in durable goods markets. Indeed, I find an increase in the direct relationship between depreciation and trading volume for aircraft models with relatively high lease rates. Together these findings suggest that adverse selection is a prominent feature of the market for contemporary used business aircraft and that leasing mitigates the consequences of asymmetric information about the quality of used durable goods.},
	number = {5},
	urldate = {2016-06-06},
	journal = {Journal of Political Economy},
	author = {Gilligan, Thomas W.},
	year = {2004},
	pages = {1157--1180},
	file = {lemons_leases.pdf:/Users/johannesmauritzen/Library/Mobile Documents/com~apple~CloudDocs/zotero/storage/J2223K8S/lemons_leases.pdf:application/pdf}
}

@techreport{hendel_adverse_1997,
	type = {Working {Paper}},
	title = {Adverse {Selection} in {Durable} {Goods} {Markets}},
	url = {http://www.nber.org/papers/w6194},
	abstract = {An undesirable feature of Akerlof style models of adverse selection is that ownership of" used cars is independent of preferences and is therefore ad hoc. We present a dynamic model" that incorporates the market for new goods. Consumers self-select into buying new or used" goods making ownership of used goods endogenous. We show that, in contrast with Akerlof and" in agreement with reality, the used market never shuts down and that the volume of trade can be" quite substantial even in cases with severe informational asymmetries. By incorporating the" market for new goods, the model lends itself to a study of the effects of adverse selection on" manufacturers' incentives. We find that manufacturers may gain from adverse selection. We" also give an example in which the market allocation under adverse selection is socially optimal. " An extension of the model to a world with many brands that differ in reliability leads to testable" predictions of the effects of adverse selection. We show that unreliable car brands have steeper" price declines and lower volumes of trade.},
	number = {6194},
	urldate = {2016-06-01},
	institution = {National Bureau of Economic Research},
	author = {Hendel, Igal and Lizzeri, Alessandro},
