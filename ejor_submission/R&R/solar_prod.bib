	month = feb,
	year = {2006},
	keywords = {Applied probability, Expert judgment, Risk analysis},
	pages = {157--177},
	file = {ScienceDirect Full Text PDF:/Users/johannesmauritzen/Library/Mobile Documents/com~apple~CloudDocs/zotero/storage/4C58B2VD/Szwed et al. - 2006 - A Bayesian paired comparison approach for relative.pdf:application/pdf;ScienceDirect Snapshot:/Users/johannesmauritzen/Library/Mobile Documents/com~apple~CloudDocs/zotero/storage/Z5GITEZB/S0377221704003911.html:text/html}
}

@article{liu_identifying_2015,
	title = {Identifying future defaulters: {A} hierarchical {Bayesian} method},
	volume = {241},
	issn = {0377-2217},
	shorttitle = {Identifying future defaulters},
	url = {http://www.sciencedirect.com/science/article/pii/S0377221714006432},
	doi = {10.1016/j.ejor.2014.08.008},
	abstract = {Traditional methods of applying classification models into the area of credit scoring may ignore the effect from censoring. Survival analysis has been introduced with its ability to deal with censored data. The mixture cure model, one important branch of survival models, is also applied in the context of credit scoring, assuming that the study population is a mixture of never-default and will-default customers. We extend the standard mixture cure model through: (1) relaxing the independence assumption of the probability and the time of default; (2) treating the missing defaulting labels as latent variables and applying an augmentation technique; and (3) introducing a discrete truncated exponential distribution to model the time of default. Our full model is written in a hierarchical form so that the Markov chain Monte Carlo method is applied to estimate corresponding parameters. Through an empirical analysis, we show that both mixture models, the standard mixture cure model and the hierarchical mixture cure model (HMCM), are more advanced in identifying future defaulters while compared with logistic regression. It is also concluded that our hierarchical Bayesian extension increases the model’s predictability and provides meaningful output for risk management.},
	number = {1},
	urldate = {2019-03-29},
	journal = {European Journal of Operational Research},
	author = {Liu, Fan and Hua, Zhongsheng and Lim, Andrew},
	month = feb,
	year = {2015},
	keywords = {Bayesian analysis, Credit scoring, Mixture cure model, Risk management},
	pages = {202--211},
	file = {ScienceDirect Full Text PDF:/Users/johannesmauritzen/Library/Mobile Documents/com~apple~CloudDocs/zotero/storage/2LB59PVH/Liu et al. - 2015 - Identifying future defaulters A hierarchical Baye.pdf:application/pdf;ScienceDirect Snapshot:/Users/johannesmauritzen/Library/Mobile Documents/com~apple~CloudDocs/zotero/storage/RQ9VJTL3/S0377221714006432.html:text/html}
}

